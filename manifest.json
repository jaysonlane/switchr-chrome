{
  "name": "switchr Chrome",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This extension was created with the awesome extensionizr.io",
  "homepage_url": "http://extensionizr.com",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "https://github.com/*",
    "tabs"
  ],
  "commands": {
    "up" : {
        "suggested_key": { "default": "MacCtrl+Ctrl+Up" },
        "description": "move up"
    },
    "down": {
        "suggested_key": { "default": "MacCtrl+Ctrl+Down" },
        "description": "move down"
    }
  }
}